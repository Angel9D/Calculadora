    public static String operaciones(String num1, String num2, String signo){
    Double resultadocalc=0.0;
    String respuesta;
    
    if(signo.equals("+")){
     resultadocalc = Double.parseDouble(num1)+Double.parseDouble(num2);
    }else  if(signo.equals("-")){
       resultadocalc = Double.parseDouble(num1)-Double.parseDouble(num2);
    }else  if(signo.equals("*")){
       resultadocalc = Double.parseDouble(num1)*Double.parseDouble(num2);
    }else  if(signo.equals("/")){
       resultadocalc = Double.parseDouble(num1)/Double.parseDouble(num2);
    }else if(signo.equals("x^n")){
       resultadocalc = Math.pow(Double.parseDouble(num1),Double.parseDouble(num2));
    }else if(signo.equals("cos")){
       resultadocalc=Math.cos(Double.parseDouble(num1));
             
    }
    respuesta=resultadocalc.toString();
    return respuesta;
    }
